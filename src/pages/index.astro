    "Linear",
    "Typescript",
    "Swelte/SwelteKit",
    "PostgreSQL",
    "Supabase",
    "React",
    "Next.js",
];

let faqs = [
    { question: "What is your design process?", answer: "..." },
    { question: "How do you handle revisions and feedback?", answer: "..." },
    { question: "Can you work with our existing dev team?", answer: "..." },
    { question: "What are your rates and payment terms?", answer: "..." },
