        }

        .marquee__item:nth-child(3n + 6) {
            border-bottom-left-radius: 1rem;
        }

        .marquee--7 .marquee__item:nth-of-type(1) {
            --marquee-item-index: 1;
        }

        .marquee--7 .marquee__item:nth-of-type(2) {
